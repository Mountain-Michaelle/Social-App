{
    "builds" : [
        {
        "src": "social_app/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python3.9"}
        },
        {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": {"distDir": "staticfiles_build"}
        }
],
    "routes": [ 
        {
            "src": "/(.*)",
            "dest": "social_app/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
       
    ]
}
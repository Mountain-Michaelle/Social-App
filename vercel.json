{
    "version": 2,
    "builds": [    
    {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "nodeVersion": "18.x"
        }
      },

      {
        "src": "social_app/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      },
      {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "social_app/wsgi.py"
      }
    ],
    "outputDirectory": "staticfiles/CACHE"
  }
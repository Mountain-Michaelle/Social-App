{
    "builds" : [
        {
        "src": "social_app/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python3.9", "buidCommand": "bash build.sh"}
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles"
            }
        }
],
    "routes": [ 
        {
            "src": "/(.*)",
            "dest": "social_app/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        }
    ]
}